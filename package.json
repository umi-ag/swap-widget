{
  "name": "umi-terminal",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "tsconfig": "workspace:*",
    "turbo": "latest"
  },
  "packageManager": "bun@1.0.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
